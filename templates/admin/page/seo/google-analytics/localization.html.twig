{% extends 'admin/page/seo/google-analytics/base.html.twig' %}

{% trans_default_domain 'admin' %}

{% block page %}

    <div class="card">
        <div class="card-title card-header">
            {{"Nombre de visites par pays"|trans}} <small class="text-muted-grey text-italic">{{"(30 derniers jours)"|trans}}</small>
        </div>
        <div class="card-body">
            <div id="geo-chart"></div>
        </div>
    </div>

    <div class="row">

        <div class="col-md-6">

            <div class="card">
                <div class="card-title card-header">
                    {{"Pourcentage du nombre de visites par pays"|trans}} <small class="text-muted-grey text-italic">{{"(30 derniers jours)"|trans}}</small>
                </div>
                <div class="card-body">
                    <div id="country-chart"></div>
                </div>
            </div>

        </div>

        <div class="col-md-6">

            <div class="card">
                <div class="card-title card-header">
                    {{"Pourcentage du nombre de visites par ville"|trans}} <small class="text-muted-grey text-italic">{{"(30 derniers jours)"|trans}}</small>
                </div>
                <div class="card-body">
                    <div id="city-chart"></div>
                </div>
            </div>

        </div>

    </div>

{% endblock %}