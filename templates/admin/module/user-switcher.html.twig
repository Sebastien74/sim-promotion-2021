{% trans_default_domain 'admin' %}

{% if is_granted('IS_IMPERSONATOR') %}
    <div class="alert alert-warning" style="margin-bottom: 0;">
        You are currently switched to this user.
        <a href="{{ app.request.uri }}?_switch_user=_exit">Exit Impersonation</a>
    </div>
{% elseif 'user-switcher'|moduleActive(allModules) %}
    <a href="{{ app.request.uri }}?_switch_user=toto@gmail.com">_switch_user</a>
{% endif %}